@Article{Zhang2014a,
  author     = {Z Zhang and L M Duan},
  title      = {Quantum metrology with Dicke squeezed states},
  journal    = {New Journal of Physics},
  year       = {2014},
  volume     = {16},
  number     = {10},
  pages      = {103037},
  abstract   = {We introduce a new class of quantum many-particle entangled states, called the Dicke squeezed (or DS) states, which can be used to improve the precision in quantum metrology beyond the standard quantum limit. We show that the enhancement in measurement precision is characterized by a single experimentally detectable parameter, called the Dicke squeezing ##IMG## [http://ej.iop.org/images/1367-2630/16/10/103037/njp502275ieqn1.gif] {${{\xi }_{D}}$} , which also bounds the entanglement depth for this class of states. The measurement precision approaches the ultimate Heisenberg limit as ##IMG## [http://ej.iop.org/images/1367-2630/16/10/103037/njp502275ieqn2.gif] {${{\xi }_{D}}$} attains the minimum in an ideal Dicke state. Compared with other entangled states, we show that the DS states are more robust to decoherence and give better measurement precision under typical experimental noise.},
  bdsk-url-1 = {http://stacks.iop.org/1367-2630/16/i=10/a=103037},
  url        = {http://stacks.iop.org/1367-2630/16/i=10/a=103037},
}

